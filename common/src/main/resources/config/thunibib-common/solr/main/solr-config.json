[
  {
    "update-requestHandler": {
      "name": "/select",
      "class": "solr.SearchHandler",
      "defaults": {
        "echoParams": "explicit",
        "defType": "edismax",
        "qs": "5",
        "mm": "100%",
        "q.op": "AND",
        "df": "all",
        "rows": 10,
        "fl": "id",
        "facet": "true",
        "facet.mincount": "1",
        "facet.field": [
          "status",
          "importID",
          "year",
          "facet_person",
          "subject",
          "genre",
          "origin_exact",
          "facet_host_title",
          "oa"
        ],
        "wt": "xml"
      }
    }
  },
  {
    "update-requestHandler": {
      "name": "/statistics",
      "class": "solr.SearchHandler",
      "defaults": {
        "echoParams": "explicit",
        "defType": "edismax",
        "qs": "5",
        "mm": "100%",
        "q.op": "AND",
        "df": "all",
        "wt": "xml"
      },
      "appends": {
        "fq": "+partOf:true",
        "facet.field": [
          "year",
          "subject",
          "genre",
          "facet_person",
          "oa"
        ]
      },
      "invariants": {
        "fl": "",
        "start": "0",
        "rows": "0",
